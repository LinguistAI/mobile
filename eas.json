{
  "cli": {
    "version": ">= 7.3.0",
    "requireCommit": true
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "ubuntu-20.04-jdk-11-ndk-r19c"
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "ios": {
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://linguistai.app/dev/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
